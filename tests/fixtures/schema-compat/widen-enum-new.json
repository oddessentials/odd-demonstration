{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$version": "1.1.0",
    "type": "object",
    "required": [
        "status"
    ],
    "properties": {
        "status": {
            "type": "string",
            "enum": [
                "PENDING",
                "RUNNING",
                "COMPLETED",
                "FAILED"
            ]
        }
    }
}