{
    "name": "gateway",
    "version": "3.4.1",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "engines": {
        "node": ">=20.0.0"
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint . --ext .ts,.js",
        "lint:fix": "eslint . --ext .ts,.js --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "dep:check": "depcruise --config .dependency-cruiser.cjs index.ts",
        "start": "node dist/index.js",
        "dev": "tsx index.ts"
    },
    "dependencies": {
        "express": "^5.2.1",
        "amqplib": "^0.10.3",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "uuid": "^9.0.1",
        "body-parser": "^1.20.2",
        "prom-client": "^15.1.0",
        "swagger-ui-express": "^5.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.0.0",
        "@types/amqplib": "^0.10.4",
        "@types/express": "^5.0.6",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "@vitest/coverage-v8": "^1.6.0",
        "dependency-cruiser": "^16.0.0",
        "eslint": "^9.0.0",
        "prettier": "^3.2.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^1.6.0",
        "@types/swagger-ui-express": "^4.1.6",
        "supertest": "^6.3.4",
        "@types/supertest": "^6.0.2"
    }
}
